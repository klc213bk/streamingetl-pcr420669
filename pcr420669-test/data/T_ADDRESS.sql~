create table T_ADDRESS (
ADDRESS_ID NUMBER(19,0),
ADDRESS_1 VARCHAR2(100),
ADDRESS_2 VARCHAR2(100),
ADDRESS_3 VARCHAR2(100),
ADDRESS_4 VARCHAR2(100),
ADDRESS_5 VARCHAR2(100),
ADDRESS_6 VARCHAR2(100),
ADDRESS_7 VARCHAR2(100),
ADDRESS_FORMAT VARCHAR2(100),
POST_CODE VARCHAR2(100),
STATE VARCHAR2(100),
COUNTRY_CODE VARCHAR2(100),
ADDRESS_STATUS VARCHAR2(100),
FOREIGN_INDI VARCHAR2(100),
UPDATE_DATE DATE, 
OPERATOR_ID NUMBER(19,0),
OPERATOR_DEPT_ID VARCHAR2(100),
INSERT_DATE  DATE,
ADDRESS_FORMAT_TYPE VARCHAR2(100),
MAIL_FAILED_TIMES NUMBER(19,0),
RETURN_MAIL_DATE DATE,
RETURN_MAIL_REASON VARCHAR2(100),
INSERT_TIME DATE,
RECORDER_ID NUMBER(19,0),
UPDATE_TIME DATE,
UPDATER_ID NUMBER(19,0),
VALIDITY_START_DATE DATE,
CITY VARCHAR2(100),
INSERT_TIMESTAMP DATE,
UPDATE_TIMESTAMP DATE,
ADDRESS_8 VARCHAR2(100),
ADDRESS_9 VARCHAR2(100),
ADDRESS_10 VARCHAR2(100),
ADDRESS_11 VARCHAR2(100),
ADDRESS_12 VARCHAR2(100),
TELEPHONE VARCHAR2(100),
FAX VARCHAR2(100),
BOX_TYPE VARCHAR2(100) ,
NATIONALITY VARCHAR2(100),
PRIMARY KEY(ADDRESS_ID)
);